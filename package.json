{
  "name": "aiapps",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered game asset generators using Gemini AI",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "type-check": "npm run type-check --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:coverage": "npm run test:coverage --workspaces --if-present",
    "test:e2e": "npm run test:e2e --workspaces --if-present",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlikebear/aiapps.git"
  },
  "keywords": [
    "ai",
    "gemini",
    "game-assets",
    "audio-generation",
    "image-generation"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devlikebear/aiapps/issues"
  },
  "homepage": "https://github.com/devlikebear/aiapps#readme",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
