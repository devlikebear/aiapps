{
  "name": "aiapps",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "AI Tools Hub - Curated collection of AI-powered creative apps",
  "author": "devlikebear",
  "license": "MIT",
  "homepage": "https://github.com/devlikebear/aiapps#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlikebear/aiapps.git"
  },
  "bugs": {
    "url": "https://github.com/devlikebear/aiapps/issues"
  },
  "keywords": [
    "ai",
    "gemini",
    "ai-apps",
    "audio-generation",
    "image-generation",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@aiapps/ui": "workspace:*",
    "@google/genai": "^1.23.0",
    "@google/generative-ai": "^0.21.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "next": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "utf-8-validate": "^6.0.5",
    "web-vitals": "^5.1.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@next/eslint-plugin-next": "^15.1.3",
    "@playwright/test": "^1.49.1",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
